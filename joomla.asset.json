{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "krbstheme",
  "version": "4.0.0",
  "description": "This file contains details of the style assets used by KR Bootstrap Theme, a site template for KR.",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "template.krbstheme.krbstheme",
      "description": "CSS for KR theme.",
      "type": "style",
      "uri": "krbstheme.min.css"
    },
    {
      "name": "template.krbstheme.krmodules",
      "description": "CSS for KR modules.",
      "type": "style",
      "uri": "krmodules.min.css"
    },
    {
      "name": "template.krbstheme.template",
      "description": "CSS for krbstheme template.",
      "type": "style",
      "uri": "template.min.css"
    },
    {
      "name": "template.krbstheme.foundation",
      "description": "CSS for krbstheme foundation.",
      "type": "style",
      "uri": "foundation.min.css"
    },
    {
      "name": "fontawesome",
      "type": "style",
      "uri": "system/joomla-fontawesome.min.css"
    },
    {
      "name": "template.krbstheme.site",
      "type": "preset",
      "dependencies": [
        "fontawesome#style",
        "template.krbstheme.foundation#style",
        "template.krbstheme.template#style",
        "template.krbstheme.krmodules#style",
        "template.krbstheme.krbstheme#style"
      ]
    }
  ]
}